{
    "args": {
        "augmentation": {
            "distort_jpeg": false,
            "do_binarize_otsu": false,
            "do_binarize_sauvola": false,
            "do_blur": false,
            "do_invert": false,
            "do_random_shear": false,
            "elastic_transform": true,
            "multiply": 1,
            "random_crop": true,
            "random_width": true
        },
        "decoding": {
            "beam_width": 10,
            "greedy": false
        },
        "depr": {
            "channels": 1
        },
        "general": {
            "batch_size": 64,
            "gpu": "0",
            "output": "output",
            "seed": 42
        },
        "learning_rate": {
            "decay_per_epoch": false,
            "decay_rate": 0.95,
            "decay_steps": -1,
            "learning_rate": 0.0003,
            "linear_decay": false,
            "optimizer": "rmsprop",
            "warmup_ratio": 0.0
        },
        "misc": {
            "normalization_file": "/path/to/normalization.json"
        },
        "model": {
            "model": "None,64,None,1 Cr3,3,24 Mp2,2,2,2 Bn Cr3,3,48 Mp2,2,2,2 Bn Cr3,3,96 Bn Cr3,3,96 Mp2,2,2,2 Bn Rc Bl512 D50 Bl512 D50 Bl512 D50 Bl512 D50 Bl512 D50 O1s92",
            "model_name": "My-Loghi-Model",
            "use_float32": false
        },
        "training": {
            "do_validate": true,
            "early_stopping_patience": 20,
            "epochs": 100,
            "max_queue_size": 256,
            "output_checkpoints": true,
            "train_list": "/path/to/train.txt",
            "training_verbosity_mode": "auto",
            "validation_list": "/path/to/val.txt"
        }
    }
}
